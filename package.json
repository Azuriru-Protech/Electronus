{
  "name": "protech-chat-desktop",
  "version": "0.0.8",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "release": "electron-builder --config electron-builder.yml"
  },
  "dependencies": {
    "@cometchat/calls-sdk-javascript": "^4.0.10",
    "@cometchat/chat-sdk-javascript": "^4.0.10",
    "@cometchat/chat-uikit-react": "^4.3.19",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.20.5",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.0",
    "electron-store": "8.2.0",
    "electron-updater": "^6.1.7",
    "emoji-picker-react": "^4.11.1",
    "html-to-image": "^1.11.11",
    "react-advanced-cropper": "^0.20.0",
    "react-i18next": "^15.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "redux": "^5.0.1",
    "sass": "^1.77.8",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "material-symbols": "^0.21.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-jsx": "^5.1.6",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
